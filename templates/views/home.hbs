                                                <label for="avatar" class="col-form-label">Avatar<sup>*</sup>:</label>
                                                <input type="file" required name="avatar" class="form-control"
                                                    id="avatar">
                                            </div>
                                            <div class="mb-3">
                                                <label for="coverImage" class="col-form-label">Cover
                                                    Image<sup>*</sup>:</label>
                                                <input type="file" required name="coverImage" class="form-control"
                                                    id="coverImage">
                                            </div>
                                            <div class="mb-3">
                                                <label for="email" class="col-form-label">Email<sup>*</sup>:</label>
                                                <input type="email" required name="email" class="form-control"
                                                    id="email">
                                            </div>
                                            <div class="mb-3">
                                                <label for="password"
                                                    class="col-form-label">Password<sup>*</sup>:</label>
                                                <input name="password" required type="password" class="form-control"
                                                    id="password">
                                            </div>
                                        </form>
                                    </div>
                                    <div class="modal-footer">
                                        <button type="button" class="btn btn-secondary"
                                            data-bs-dismiss="modal">Cancel</button>
                                        <button type="submit" form="signupForm" class="btn btn-primary">Singup</button>
                                    </div>
                                </div>
                            </div>
